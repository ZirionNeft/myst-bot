{
  "name": "myst_bot",
  "version": "1.0.0",
  "description": "Myst Bot",
  "main": "build/index.js",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'babel-node' build/index.js",
    "start": "node build/index.js",
    "clean": "rm -rf build",
    "build": "yarn clean && tsc",
    "migrate": "yarn build && node_modules/.bin/sequelize db:migrate --debug --env development",
    "migrate:undo": "yarn build && node_modules/.bin/sequelize db:migrate:undo --env development",
    "migrate:undo:all": "yarn build && node_modules/.bin/sequelize db:migrate:undo:all --env development",
    "app:upgrade": "yarn install && yarn migrate",
    "postinstall": "node-config-ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZirionNeft/myst-bot.git"
  },
  "author": "ZirionNeft",
  "license": "ISC",
  "dependencies": {
    "@type-cacheable/core": "^6.2.0",
    "@type-cacheable/node-cache-adapter": "^6.2.0",
    "@typeit/discord": "^4.0.8",
    "discord-paginationembed": "^2.1.0",
    "discord.js": "^12.2.0",
    "dotenv": "^8.2.0",
    "node-cache": "^5.1.2",
    "node-config-ts": "^3.0.4",
    "pg": "^8.3.0",
    "pg-hstore": "^2.3.3",
    "pino": "^6.5.1",
    "pino-pretty": "^4.1.0",
    "sequelize": "^6.3.1",
    "typescript-ioc": "^3.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/node": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@types/node": "^14.0.22",
    "@types/pino": "^6.3.0",
    "@types/validator": "^13.1.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "sequelize-cli": "^6.2.0",
    "tslib": "^2.0.0",
    "typescript": "^3.9.6"
  }
}
